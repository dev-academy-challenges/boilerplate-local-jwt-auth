{
  "name": "boilerplate-local-jwt-auth",
  "version": "0.3.0",
  "description": "Boilerplate using libsodium and JWTs",
  "main": "server/index.js",
  "author": "EDA",
  "license": "ISC",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "dev": "nodemon --watch server & webpack --watch",
    "knex": "knex",
    "migrate": "[ \"$NODE_ENV\" = \"production\" ] && exit 0; knex migrate:latest",
    "migrate:prod": "knex migrate:latest",
    "postinstall": "webpack && npm run migrate",
    "start": "node server",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "dotenv": "^7.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.19.5",
    "pg": "^7.18.1",
    "superagent": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "jest": "^25.1.0",
    "libsodium-wrappers": "^0.7.4",
    "nodemon": "^2.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.2.0",
    "sqlite3": "^4.0.6",
    "supertest": "^4.0.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-livereload-plugin": "^2.2.0"
  }
}
